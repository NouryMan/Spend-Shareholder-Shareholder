@model Spend.Models.ACCH_PROJECT_Model

@{
    ViewBag.Title = "Create";


}



<input type="hidden" value="3" id="suiteCode" />
<input type="hidden" value="1" id="projectCode" />


<div class="dashboard-content-wrap">
    <div class="dashboard-bread dashboard--bread dashboard-bread-2">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title font-size-30 text-white">انشاء مشروع</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                    <div class="breadcrumb-list text-right">
                        <ul class="list-items">
                            <li><a href="~" class="text-white">الصفحة الرئيسية</a></li>
                            <li>ادارة المشاريع </li>
                            <li>ادارة المشاريع @ViewBag.PARENT_NAME</li>

                        </ul>
                    </div><!-- end breadcrumb-list -->
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div>
    </div><!-- end dashboard-bread -->
    <div class="dashboard-main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <div>
                                <h3 class="title"> بطاقة مشروع</h3>
                                @*<p class="font-size-14">إظهار 1 إلى 10 من أصل 20 مُدخل</p>*@
                            </div>



                        </div>
                        <div class="form-content">


                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })



                                <div class="row mb-3">

                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">تكويد المشروع</label>
                                            <input type="hidden" class="form-control" id="PROJECT_NO" name="PROJECT_NO" value="@Model.PROJECT_NO" />
                                            <input type="hidden" class="form-control" id="ID" name="ID" value="@Model.ID" />
                                            <input type="hidden" class="form-control" id="IMAGE_PATH" name="IMAGE_PATH" value="@Model.IMAGE_PATH" />
                                            <input type="hidden" class="form-control" id="PROJECT_CODE" name="PROJECT_CODE" value="@Model.PROJECT_CODE" />
                                            <span class="form-control">@Model.PROJECT_NO</span>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">المشروع الرئيسي</label>
                                            <input type="hidden" id="PROJECT_PARENT_ID" name="PROJECT_PARENT_ID" value="@Model.PROJECT_PARENT_ID" />
                                            <span class="form-control">
                                                @if (Model.PROJECT_PARENT_ID > 0)
                                                {@Model.ProjectModels.PROJECT_AR_NAME}
                                               </span>
                                            
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">نوع المشروع</label>
                                            <input name="PROJECT_TYPE_ID" id="PROJECT_TYPE_ID" class="form-control" type="hidden" value="@Model.PROJECT_TYPE_ID" />
                                          
                                            @if (Model.PROJECT_TYPE_ID == 3)
                                            {

                                                <span class="form-control">وحدة </span>
                                            }
                                            else if (Model.PROJECT_TYPE_ID == 2)
                                            { <span class="form-control">عقار </span>
                                            }
                                            else
                                            { <span class="form-control">مشروع </span>}
                                        </div>
                                    </div>


                                </div>






                                <div class="row mb-3">
                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">الإسم العربي</label>
                                            @Html.EditorFor(model => model.PROJECT_AR_NAME, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " الإسم العربي", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">الإسم الانجليزي</label>
                                            @Html.EditorFor(model => model.PROJECT_EN_NAME, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " الإسم الانجليزي", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>


                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">

                                            <label for="inputPasswordConfirm">العنوان</label>
                                            @Html.EditorFor(model => model.PROJECT_LOCATION, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " العنوان", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>




                                </div>



                                <div class="row mb-3">
                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم المخطط</label>
                                            @Html.EditorFor(model => model.PLANNED_NO, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " رقم المخطط", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم القطعة</label>
                                            @Html.EditorFor(model => model.PIECE_NO, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " رقم القطعة", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>


                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">

                                            <label for="inputPasswordConfirm">الصورة</label>

                                            <input type="file" name="file" class="form-control" />
                                            <span asp-validation-for="IMAGE_PATH" class="text-danger"></span>

                                        </div>
                                    </div>




                                </div>


                                <div class="row mb-3">
                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم الصك</label>
                                            @Html.EditorFor(model => model.DEED_NO, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "رقم الصك", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">مكان الاصدار</label>
                                            @Html.EditorFor(model => model.ISSUER, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " مكان الاصدار", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>


                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">

                                            <label for="inputPasswordConfirm">تاريخ الاصدار</label>
                                            @Html.EditorFor(model => model.DEED_DATE, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " تاريخ الاصدار", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>




                                </div>

                                <div class="row mb-3">
                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">المساحة</label>
                                            @Html.EditorFor(model => model.SUITE_SPACE, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "المساحة", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-4">
                                        <div class="form-floating mb-6 mb-md-0">
                                            <label for="inputPasswordConfirm">اجمالي عدد الغرف</label>
                                            @Html.EditorFor(model => model.ROOM_COUNT, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "اجمالي عدد الغرف", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>


                                    <div class="col-md-4">
                                        <div class="form-floating mb-12 mb-md-0">

                                            <label for="inputPasswordConfirm">ملاحظة</label>
                                            @Html.EditorFor(model => model.NOTE, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " ملاحظة", } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })
                                        </div>
                                    </div>




                                </div>






                                <div class="row mt-4 mb-0">
                                    <div class="d-grid col-md-6 "><button class="btn btn-primary btn-block" type="submit">حفظ</button></div>

                                    <div class="d-grid col-md-6"><a class="btn btn-danger btn-block" href="~/ACC_HOLDER/Index">رجوع</a></div>





                                </div>
                            }

                        </div>
                    </div><!-- end form-box -->
                </div><!-- end col-lg-12 -->
            </div><!-- end row -->

            <div class="border-top mt-5"></div>
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="copy-right padding-top-30px">

                        <p class="copy__desc">
                            &copy; Copyright Tech Light 2022. Made with
                            <span class="la la-heart"></span> by <a href="http://TechLightsa.com/">Noury</a>
                        </p>

                    </div><!-- end copy-right -->
                </div><!-- end col-lg-7 -->
                <div class="col-lg-5">
                    <div class="footer-social-box text-right padding-top-30px">
                        <ul class="social-profile">
                            <li><a href="#"><i class="lab la-facebook-f"></i></a></li>
                            <li><a href="#"><i class="lab la-twitter"></i></a></li>
                            <li><a href="#"><i class="lab la-instagram"></i></a></li>
                            <li><a href="#"><i class="lab la-linkedin-in"></i></a></li>
                        </ul>
                    </div><!-- end copy-right-content -->
                </div><!-- end col-lg-5 -->
            </div><!-- end row -->
        </div><!-- end container-fluid -->
    </div><!-- end dashboard-main-content -->
</div><!-- end dashboard-content-wrap -->
