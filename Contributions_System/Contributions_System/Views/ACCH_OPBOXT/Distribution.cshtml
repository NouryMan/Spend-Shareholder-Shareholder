
@model Spend.Api.Models.API_TRANSESALE_INVO_Model
@{
    ViewBag.Title = "توزيع ارباح";
}









<div class="dashboard-content-wrap">
    <div class="dashboard-bread dashboard--bread dashboard-bread-2">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title font-size-30 text-white">توزيع ارباح</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                    <div class="breadcrumb-list text-right">
                        <ul class="list-items">
                            <li><a href="~" class="text-white">الصفحة الرئيسية</a></li>
                            <li>التوزيع </li>
                            <li>توزيع ارباح</li>

                        </ul>
                    </div><!-- end breadcrumb-list -->
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div>
    </div><!-- end dashboard-bread -->
    <div class="dashboard-main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <div>
                                <h3 class="title"> توزيع  </h3>
                                @*<p class="font-size-14">إظهار 1 إلى 10 من أصل 20 مُدخل</p>*@
                            </div>



                        </div>
                        <div class="form-content">


                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })

                                <div class="row mb-3">
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">اسم المشروع</label>
                                            <select id="ProjNo" name="ProjNo" dir="ltr" class="form-control selectpicker " required data-live-search="true">
                                                <option value="">اختار مشروع</option>
                                                @foreach (var p in ViewBag.Project)
                                                {
                                                    if (p.PROJECT_NO == ViewBag.PROJECT_NO)
                                                    {
                                                        <option selected class="themeprimary" value='@p.ID'>
                                                            @p.PROJECT_AR_NAME
                                                        </option>
                                                    }
                                                    else
                                                    {
                                                        <option class="themeprimary" value='@p.ID'>
                                                            @p.PROJECT_AR_NAME
                                                        </option>
                                                    }



                                                }

                                            </select>
                                          
                                        </div>
                                    </div>


                                    <div class="col-md-6">
                                       
                                            <div class="form-floating mb-12 mb-md-0">
                                                <label for="inputPasswordConfirm">مبلغ الصيانة</label>
                                                @Html.EditorFor(model => model.Mantinc, new { htmlAttributes = new { @class = "form-control", @type = "number", placeholder = "مبلغ التسويق", @required = "required", } })
                                                @Html.ValidationMessageFor(model => model.Mantinc, "", new { @class = "text-danger" })

                                            </div>
                                        
                                       
                                    </div>


                                </div>



                                <div class="row mb-3">
                                    <div class="col-md-6">
                                       
                                            <div class="form-floating mb-12 mb-md-0">
                                                <label for="inputPasswordConfirm">مبلغ التسويق</label>
                                                @Html.EditorFor(model => model.Markting, new { htmlAttributes = new { @class = "form-control", @type = "number", placeholder = "مبلغ التسويق", @required = "required", } })
                                                @Html.ValidationMessageFor(model => model.Mantinc, "", new { @class = "text-danger" })

                                            </div>
                                        

                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">صرفيات اخرى</label>
                                            @Html.EditorFor(model => model.OtherCost, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "صرفيات اخرى", @required = "required" } })
                                            @Html.ValidationMessageFor(model => model.OtherCost, "", new { @class = "text-danger" })

                                        </div>
                                    </div>



                                </div>

                                <div class="row mb-3">

                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">مبلغ التوزيع على المساهمين</label>
                                            @Html.EditorFor(model => model.PurCost, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = " مبلغ التوزيع على المساهمين", } })
                                            @Html.ValidationMessageFor(model => model.PurCost, "", new { @class = "text-danger" })

                                        </div>
                                    </div>
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">البيان</label>
                                            @Html.EditorFor(model => model.Note, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " البيان" } })
                                            @Html.ValidationMessageFor(model => model.Note, "", new { @class = "text-danger" })

                                        </div>
                                    </div>
                                   

                                </div>










                                <div class="row mt-4 mb-0">
                                    <div class="d-grid col-md-6 "><button class="btn btn-primary btn-block" type="submit">حفظ</button></div>

                                    <div class="d-grid col-md-6"><a class="btn btn-danger btn-block" href="~/ACC_HOLDER/Index">رجوع</a></div>





                                </div>
                            }

                        </div>
                    </div><!-- end form-box -->
                </div><!-- end col-lg-12 -->
            </div><!-- end row -->

            <div class="border-top mt-5"></div>
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="copy-right padding-top-30px">

                        <p class="copy__desc">
                            &copy; Copyright Tech Light 2022. Made with
                            <span class="la la-heart"></span> by <a href="http://TechLightsa.com/">Noury</a>
                        </p>

                    </div><!-- end copy-right -->
                </div><!-- end col-lg-7 -->
                <div class="col-lg-5">
                    <div class="footer-social-box text-right padding-top-30px">
                        <ul class="social-profile">
                            <li><a href="#"><i class="lab la-facebook-f"></i></a></li>
                            <li><a href="#"><i class="lab la-twitter"></i></a></li>
                            <li><a href="#"><i class="lab la-instagram"></i></a></li>
                            <li><a href="#"><i class="lab la-linkedin-in"></i></a></li>
                        </ul>
                    </div><!-- end copy-right-content -->
                </div><!-- end col-lg-5 -->
            </div><!-- end row -->
        </div><!-- end container-fluid -->
    </div><!-- end dashboard-main-content -->
</div><!-- end dashboard-content-wrap -->






<script>
    function getAcc_NO() {



        var id = $('#ACC_PARENT').val();
        $('#parent-Acc').val(id);

        var ajaxMethodUrl = "getAcc_NO?parent_id=" + id;

        $.ajax({
            async: false,
            cache: true,
            type: "Get",
            url: ajaxMethodUrl,
            success: function (data) {
                $('#Acc_NOString').val(data);


            },


        });
    }



</script>