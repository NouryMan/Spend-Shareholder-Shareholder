@model Spend.Models.ACCH_PROJ_PERCENTTBL_Model
@{
    ViewBag.Title = "توزيع نسب المساهمة الختامية";
}









<div class="dashboard-content-wrap">
    <div class="dashboard-bread dashboard--bread dashboard-bread-2">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title font-size-30 text-white">توزيع نسب المساهمة الختامية</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                    <div class="breadcrumb-list text-right">
                        <ul class="list-items">
                            <li><a href="~" class="text-white">الصفحة الرئيسية</a></li>
                            <li>ادارة المشاريع </li>
                            <li>المساهمين</li>

                        </ul>
                    </div><!-- end breadcrumb-list -->
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div>
    </div><!-- end dashboard-bread -->
    <div class="dashboard-main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <div>
                                <h3 class="title"> توزيع نسب المساهمة الختامية</h3>
                                @*<p class="font-size-14">إظهار 1 إلى 10 من أصل 20 مُدخل</p>*@
                            </div>



                        </div>
                        <div class="form-content">


                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })




                            <div class="row mb-3">
                                <div class="col-md-6">

                                    <div class="form-floating mb-12 mb-md-0">
                                        <label for="inputPasswordConfirm">المشروع</label>
                                        <select id="PROJ_NO" name="PROJ_NO" dir="ltr" class="form-control selectpicker " onchange="Show()" data-live-search="true" required>
                                            <option value="">اختار المشروع</option>
                                            @foreach (var p in ViewBag.PROJ)
                                            {
                                                <option class="themeprimary" value="@p.PROJECT_NO"> @p.PROJECT_NAME</option>
                                            }

                                        </select>


                                    </div>

                                </div>


                                <div class="col-md-6">
                                    <div class="form-floating">
                                        <label for="inputLastName">الصندوق</label>
                                        <select id="BOX_NO" name="BOX_NO" dir="ltr" class="form-control selectpicker " data-live-search="true">
                                            <option value="">اختار الصندوق</option>
                                            @foreach (var p in ViewBag.Box)
                                            {
                                                try
                                                {
                                                    <option class="themeprimary" value="@p.BOX_NO"> @p.BOX_NAME</option>
                                                }
                                                catch { }
                                            }

                                        </select>

                                    </div>
                                </div>


                            </div>

                                <div class="row mb-3">
                                    <div class="col-md-3">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">المبلغ المخصص لصيانة</label>
                                            @Html.EditorFor(model => model.MAINTE_PERCENT, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "" } })
                                            @Html.ValidationMessageFor(model => model.MAINTE_PERCENT, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">المبلغ المخصص لتسويق</label>
                                            @Html.EditorFor(model => model.MARKETING_PERCENT, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "" } })
                                            @Html.ValidationMessageFor(model => model.MARKETING_PERCENT, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">المبلغ المخصص لمصاريف اخراء</label>
                                            @Html.EditorFor(model => model.OTHER_COST, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "" } })
                                            @Html.ValidationMessageFor(model => model.OTHER_COST, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-3">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">نسبة التشغيل</label>
                                            @Html.EditorFor(model => model.WORK_PERCENT, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "" } })
                                            @Html.ValidationMessageFor(model => model.WORK_PERCENT, "", new { @class = "text-danger" })

                                        </div>
                                    </div>



                                </div>

                                <div class="row mb-3">


                                  

                                    <div class="col-md-12">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">ملاحظة</label>
                                            @Html.EditorFor(model => model.NOTE, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "" } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>





                                </div>



                                <div class="row mt-4 mb-0">
                                    <div class="d-grid col-md-6 "><button class="btn btn-primary btn-block" type="submit">حفظ</button></div>

                                    <div class="d-grid col-md-6"><a class="btn btn-danger btn-block" href="~/ACC_HOLDER/Index">رجوع</a></div>





                                </div>
                            }

                        </div>
                    </div><!-- end form-box -->
                </div><!-- end col-lg-12 -->
            </div><!-- end row -->

            <div class="border-top mt-5"></div>
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="copy-right padding-top-30px">

                        <p class="copy__desc">
                            &copy; Copyright Tech Light 2022. Made with
                            <span class="la la-heart"></span> by <a href="http://TechLightsa.com/">Noury</a>
                        </p>

                    </div><!-- end copy-right -->
                </div><!-- end col-lg-7 -->
                <div class="col-lg-5">
                    <div class="footer-social-box text-right padding-top-30px">
                        <ul class="social-profile">
                            <li><a href="#"><i class="lab la-facebook-f"></i></a></li>
                            <li><a href="#"><i class="lab la-twitter"></i></a></li>
                            <li><a href="#"><i class="lab la-instagram"></i></a></li>
                            <li><a href="#"><i class="lab la-linkedin-in"></i></a></li>
                        </ul>
                    </div><!-- end copy-right-content -->
                </div><!-- end col-lg-5 -->
            </div><!-- end row -->
        </div><!-- end container-fluid -->
    </div><!-- end dashboard-main-content -->
</div><!-- end dashboard-content-wrap -->



