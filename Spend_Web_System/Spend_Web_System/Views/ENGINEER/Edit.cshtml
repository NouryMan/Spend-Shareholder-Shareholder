@model Spend.Models.ENGINEERTBL_Model
@{
    ViewBag.Title = " بطاقة مهندس ";
}



<div class="dashboard-content-wrap">
    <div class="dashboard-bread dashboard--bread dashboard-bread-2">
        <div class="container-fluid">
            <div class="row align-items-center">
                <div class="col-lg-6">
                    <div class="breadcrumb-content">
                        <div class="section-heading">
                            <h2 class="sec__title font-size-30 text-white">تعديل مهندس</h2>
                        </div>
                    </div><!-- end breadcrumb-content -->
                </div><!-- end col-lg-6 -->
                <div class="col-lg-6">
                    <div class="breadcrumb-list text-right">
                        <ul class="list-items">
                            <li><a href="index.html" class="text-white">الصفحة الرئيسية</a></li>

                            <li>المشاريع</li>
                            <li>تعديل مهندس</li>
                        </ul>
                    </div><!-- end breadcrumb-list -->
                </div><!-- end col-lg-6 -->
            </div><!-- end row -->
        </div>
    </div><!-- end dashboard-bread -->
    <div class="dashboard-main-content">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-12">
                    <div class="form-box">
                        <div class="form-title-wrap">
                            <div>
                                <h3 class="title"> بطاقة مهندس</h3>
                                @*<p class="font-size-14">إظهار 1 إلى 10 من أصل 20 مُدخل</p>*@
                            </div>



                        </div>
                        <div class="form-content">


                            @using (Html.BeginForm())
                            {
                                @Html.AntiForgeryToken()

                                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                                @Html.HiddenFor(model => Model.ENG_NO);


                                <div class="row mb-3">
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم المهندس</label>
                                            @Html.EditorFor(model => model.ENG_NO, new { htmlAttributes = new { @class = "form-control", @type = "text", @disabled = "disabled", @placeholder = "الاسم" } })
                                            @Html.ValidationMessageFor(model => model.ENG_NO, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm"> الاسم</label>
                                            @Html.EditorFor(model => model.ENG_NAME, new { htmlAttributes = new { @class = "form-control", @type = "text", @required = "required", @placeholder = "الاسم" } })
                                            @Html.ValidationMessageFor(model => model.ENG_NAME, "", new { @class = "text-danger" })

                                        </div>
                                    </div>


                                </div>

                                <div class="row mb-3">
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm"> العنوان</label>
                                            @Html.EditorFor(model => model.ADDRESS, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = "العنوان" } })
                                            @Html.ValidationMessageFor(model => model.ADDRESS, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">الجنسية</label>
                                            @Html.EditorFor(model => model.NATIONALITY, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " الجنسية" } })
                                            @Html.ValidationMessageFor(model => model.NATIONALITY, "", new { @class = "text-danger" })

                                        </div>
                                    </div>


                                </div>
                                <div class="row mb-3">
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم الجوال</label>
                                            @Html.EditorFor(model => model.PHONE_NO, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "رقم الجوال" } })
                                            @Html.ValidationMessageFor(model => model.PHONE_NO, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">رقم الموبايل</label>
                                            @Html.EditorFor(model => model.MOBILE_NO, new { htmlAttributes = new { @class = "form-control", @type = "number", @placeholder = "رقم الموبايل" } })
                                            @Html.ValidationMessageFor(model => model.MOBILE_NO, "", new { @class = "text-danger" })

                                        </div>
                                    </div>


                                </div>
                                <div class="row mb-3">
                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm"> الوظيفة</label>
                                            @Html.EditorFor(model => model.JOB, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " الوظيفة" } })
                                            @Html.ValidationMessageFor(model => model.JOB, "", new { @class = "text-danger" })

                                        </div>
                                    </div>

                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">المؤهل</label>
                                            @Html.EditorFor(model => model.QUALIFICATION, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " المؤهل" } })
                                            @Html.ValidationMessageFor(model => model.QUALIFICATION, "", new { @class = "text-danger" })

                                        </div>
                                    </div>


                                </div>
                                <div class="row mb-3">


                                    <div class="col-md-6">
                                        <div class="form-floating mb-12 mb-md-0">
                                            <label for="inputPasswordConfirm">ملاحظة</label>
                                            @Html.EditorFor(model => model.NOTE, new { htmlAttributes = new { @class = "form-control", @type = "text", @placeholder = " ملاحظة" } })
                                            @Html.ValidationMessageFor(model => model.NOTE, "", new { @class = "text-danger" })

                                        </div>
                                    </div>


                                </div>



                                <div class="row mt-4 mb-0">
                                    <div class="d-grid col-md-6 "><button class="btn btn-primary btn-block" type="submit">حفظ</button></div>

                                    <div class="d-grid col-md-6"><a class="btn btn-danger btn-block" href="~/ENGINEER/Index">رجوع</a></div>





                                </div>
                            }

                        </div>
                    </div><!-- end form-box -->
                </div><!-- end col-lg-12 -->
            </div><!-- end row -->

            <div class="border-top mt-5"></div>
            <div class="row align-items-center">
                <div class="col-lg-7">
                    <div class="copy-right padding-top-30px">

                        <p class="copy__desc">
                            &copy; Copyright Tech Light 2022. Made with
                            <span class="la la-heart"></span> by <a href="http://Tech-Light.com/">Noury</a>
                        </p>

                    </div><!-- end copy-right -->
                </div><!-- end col-lg-7 -->
                <div class="col-lg-5">
                    <div class="footer-social-box text-right padding-top-30px">
                        <ul class="social-profile">
                            <li><a href="#"><i class="lab la-facebook-f"></i></a></li>
                            <li><a href="#"><i class="lab la-twitter"></i></a></li>
                            <li><a href="#"><i class="lab la-instagram"></i></a></li>
                            <li><a href="#"><i class="lab la-linkedin-in"></i></a></li>
                        </ul>
                    </div><!-- end copy-right-content -->
                </div><!-- end col-lg-5 -->
            </div><!-- end row -->
        </div><!-- end container-fluid -->
    </div><!-- end dashboard-main-content -->
</div><!-- end dashboard-content-wrap -->






